ter();\r\n\r\n    const chatBasla=()=>{\r\n      router.push({name:'adminhome'})\r\n    }\r\n\r\n    return {uyeMi,chatBasla}\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#nt{\r\n text-align: center;\r\n font-size: 30px;\r\n margin-top: 100px;\r\n\r\n}\r\n#uye{\r\n  text-align: center;\r\n}\r\n#grs{\r\n  text-align: center;\r\n}\r\nbody{\r\n  background-color: cadetblue;\r\n}\r\n#resim{\r\n margin-left: 650px;\r\n}\r\n.home1{\r\n  text-align: left;\r\n    box-shadow: 10px 10px 10px rgb(220, 204, 204);\r\n  background: rgb(241, 237, 237);\r\n\r\n\r\n}\r\nspan {\r\n  font-weight: bold;\r\n  text-decoration: underline;\r\n  color: #b33939;\r\n  cursor: pointer;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}